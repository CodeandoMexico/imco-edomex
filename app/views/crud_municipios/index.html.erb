<h1>Listing crud_municipios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @crud_municipios.each do |crud_municipio| %>
      <tr>
        <td><%= crud_municipio.nombre %></td>
        <td><%= link_to 'Show', crud_municipio %></td>
        <td><%= link_to 'Edit', edit_crud_municipio_path(crud_municipio) %></td>
        <td><%= link_to 'Destroy', crud_municipio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Crud municipio', new_crud_municipio_path %>
